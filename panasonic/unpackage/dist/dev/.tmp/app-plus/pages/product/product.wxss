.productpage {
  background: #FFFFFF;
}
#overview .swiper {
  height: 300rpx;
}
#overview .overviewgallery {
  width: 100%;
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  -webkit-align-content: center;
      -ms-flex-line-pack: center;
          align-content: center;
}
#overview image {
  height: 273rpx;
  width: 364rpx;
}
#overview .productimg {
  text-align: center;
}
.productname {
  text-align: center;
  font-size: 24rpx;
  margin-top: 24rpx;
}
#feature {
  margin-top: 30rpx;
  overflow: hidden;
}
#feature .swiper {
    height: 293.75rpx;
}
#feature .galleryfeature {
    position: relative;
}
#feature image {
    width: 750rpx;
    height: 293.75rpx;
}
#feature .swiper-dots {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    position: absolute;
    right: 20rpx;
    bottom: 12rpx;
    width: 60rpx;
    height: 30rpx;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
}
#feature .swiper-dots .num {
      width: 30rpx;
      height: 30rpx;
      border-radius: 50px;
      font-size: 18rpx;
      color: #fff;
      text-align: center;
      line-height: 30rpx;
}
#feature .swiper-dots .sign {
      position: absolute;
      top: 0;
      left: 50%;
      line-height: 30rpx;
      font-size: 12rpx;
      color: #fff;
      -webkit-transform: translateX(-50%);
          -ms-transform: translateX(-50%);
              transform: translateX(-50%);
}
#feature .sliderbutton {
    position: absolute;
    top: 38%;
    width: 100%;
    z-index: 20;
    height: 0;
}
#feature .sliderbutton .uni-icon {
      background: rgba(164, 164, 164, 0.7);
      height: 50rpx;
      line-height: 50rpx;
      width: 50rpx;
      text-align: center;
      border-radius: 52rpx;
      color: #ffffff;
}
#feature .sliderbutton .uni-icon-arrowleft {
      float: left;
      -webkit-transform: translateX(-40%);
          -ms-transform: translateX(-40%);
              transform: translateX(-40%);
}
#feature .sliderbutton .uni-icon-arrowright {
      float: right;
      -webkit-transform: translateX(40%);
          -ms-transform: translateX(40%);
              transform: translateX(40%);
}
#spectable {
  margin: 50rpx 20rpx 0;
}
#spectable .title {
    font-size: 24rpx;
    margin-bottom: 20rpx;
}
#related {
  margin: 30rpx 0 0;
  background: #f6f6f6;
  padding: 32rpx 20rpx 0;
}
#related .title {
    font-size: 24rpx;
}
.scroll-view_H {
  white-space: nowrap;
  width: 100%;
  margin-top: 25rpx;
}
.scroll-view-item_H {
  display: inline-block;
}
.uni-icon-arrowleft:before {
  content: '\E582';
}
.uni-icon-arrowright:before {
  content: '\E583';
}
.product-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-flex-wrap: wrap;
      -ms-flex-wrap: wrap;
          flex-wrap: wrap;
  margin: 20rpx -5rpx;
}

